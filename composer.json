{
    "name": "chaplean/ovh-logs-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Provides support for sending logs to OVH's graylog",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "https://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\OvhLogsBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\OvhLogsBundle\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "graylog2/gelf-php": "^1.5",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/console": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/monolog-bundle": "^3.0 || ^4.0",
        "symfony/yaml": "^3.0 || ^4.0"
    },
    "require-dev": {
        "chaplean/coding-standard": "^1.1",
        "mockery/mockery": "dev-master",
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.3",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0",
        "symfony/var-dumper": "^2.8 || ^3.0 || ^4.0"
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    }
}
